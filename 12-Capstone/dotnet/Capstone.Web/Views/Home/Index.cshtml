@model IList<Park>
@{
    ViewData["Title"] = "Home Page";
}

<div id="parksSection" class="container">
    @foreach (Park park in Model)
    {
        <div>
            <a asp-controller="Home" asp-action="Detail" asp-route-parkCode="@park.ParkCode">
                <img class="img-responsive" src="~/images/parks/@(park.ParkCode.ToLower()).jpg" />
            </a>
            <div>
                <h3>@park.ParkName</h3>
                <p>@park.ParkDescription</p>
            </div>
        </div>
    }
</div>